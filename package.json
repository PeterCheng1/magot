{
  "name": "@billjs/magot",
  "version": "1.0.0",
  "description": "A cool and easy UI framework of customizable themes and React-based.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "es",
    "lib",
    "release",
    "lib.d.ts",
    "tsconfig.json"
  ],
  "scripts": {
    "prepublish": "npm run lint && npm run build",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -r @babel/register --config webpack.config.dev.js --progress --colors",
    "build:es": "node -r @babel/register scripts/build/es.js",
    "build:lib": "node -r @babel/register scripts/build/lib.js",
    "build:release": "node -r @babel/register scripts/build/release.js",
    "compile": "npm run build:es && npm run build:lib || exit 1",
    "build": "npm run build:es && npm run build:lib && npm run build:release || exit 1",
    "fix:prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,less,json,yml,yaml,md,mdx}\"",
    "fix:ts": "tslint -p ./tsconfig.json --type-check --fix",
    "fix:js": "eslint --ext .js,.jsx ./ --cache --quiet --fix",
    "fix:style": "stylelint \"src/**/*.less\" --syntax less --fix",
    "fix": "npm run fix:prettier && npm run fix:ts && npm run fix:js && npm run fix:style || exit 1",
    "lint:prettier": "prettier --list-different \"**/*.{ts,tsx,js,jsx,less,json,yml,yaml,md,mdx}\"",
    "lint:ts": "tslint -p ./tsconfig.json --type-check",
    "lint:js": "eslint --ext .js,.jsx ./ --cache --quiet",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "npm run lint:prettier && npm run lint:ts && npm run lint:js && npm run lint:style || exit 1",
    "docz:dev": "cross-env DEBUG=true docz dev",
    "docz:build": "docz build",
    "create:component": "node -r @babel/register scripts/creator/component/index.js",
    "test": "jest --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/billjs/magot.git"
  },
  "keywords": [
    "Magot",
    "UI",
    "React",
    "Theme"
  ],
  "contributors": [
    "billjs"
  ],
  "author": "billjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/billjs/magot/issues"
  },
  "homepage": "https://billjs.github.io/magot/",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@emotion/core": "^10.0.9",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.0",
    "@types/jest": "^24.0.11",
    "@types/less": "^3.0.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^11.11.3",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.1",
    "@types/shelljs": "^0.8.3",
    "@types/sinon": "^7.0.10",
    "@types/webpack": "^4.4.25",
    "@types/webpack-dev-server": "^3.1.4",
    "@types/webpack-merge": "^4.1.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "docz": "^0.13.7",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^0.13.7",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.15.2",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.1",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.5",
    "mkdirp": "^0.5.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-each": "^0.10.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
