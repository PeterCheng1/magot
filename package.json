{
  "name": "@billjs/magot",
  "version": "1.0.0",
  "description": "A cool and easy UI framework of customizable themes and React-based.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "es",
    "lib",
    "release"
  ],
  "scripts": {
    "prepublish": "npm run lint && npm run build",
    "start": "npm run dev",
    "dev": "webpack-dev-server -r @babel/register --config webpack.config.dev.js --progress --colors",
    "build:es": "node -r @babel/register scripts/build/es.js",
    "build:lib": "node -r @babel/register scripts/build/lib.js",
    "build:release": "cross-env NODE_ENV=production webpack -r @babel/register --config webpack.config.js --progress",
    "build": "npm run build:es && npm run build:lib && npm run build:release || exit 1",
    "lint:ts": "tslint -p ./tsconfig.json --type-check",
    "lint:js": "eslint --ext .js,.jsx ./ --cache --quiet",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "npm run prettier && npm run lint:ts && npm run lint:js && npm run lint:style || exit 1",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/billjs/magot.git"
  },
  "keywords": [
    "Magot",
    "UI",
    "React",
    "Theme"
  ],
  "contributors": [
    "billjs"
  ],
  "author": "billjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/billjs/magot/issues"
  },
  "homepage": "https://billjs.github.io/magot/",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@types/classnames": "^2.2.7",
    "@types/less": "^3.0.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/shelljs": "^0.8.1",
    "@types/webpack": "^4.4.22",
    "@types/webpack-dev-server": "^3.1.1",
    "@types/webpack-merge": "^4.1.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "glob": "^7.1.3",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "mkdirp": "^0.5.1",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "shelljs": "^0.8.3",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.2.0",
    "classnames": "^2.2.6"
  },
  "peerDependencies": {
    "react": ">=16.7.0",
    "react-dom": ">=16.7.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
